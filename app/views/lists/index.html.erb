<% @lists.each do |list| %>

  <div class="row">
    <div class="col-md-6 col-md-offset-3"> 
      <%= list.id %>
      <%= link_to list.title, list_path(:id => list.id)%> 

      <%= list.user_id %>
      <%= link_to "New", new_list_path %> <%= link_to "Edit", edit_list_path(:id => list.id) %> <%= link_to "Delete", '#'%>
</div>
</div>
<% end %>

