My Todo List Index

<% @lists.each do |list| %>
<div class="row">
    <div class="col-md-4"> 
    <%= hidden_field :id, :value => list.id %>
    <%= list.title %> 
    </div>
    <div class="col-mod-4">
    <%= list.created_at %>
    </div>

    <div class="col-md-4"> 
    <%= link_to "See todo items", list_path(:id => list.id)%> 
    <%= link_to "Edit", edit_list_path(list), method: "get" %>
    <%= link_to "Delete", list_path(list), method: :delete, data: {confirm: "You sure?"} %>
  </div>
</div>
<% end %>
<div class="row">
    <div class="col-md-6 col-md-offset-3"> 
<%= link_to "Create New Todo", new_list_path, class: "btn btn-primary" %>
</div>
</div>
