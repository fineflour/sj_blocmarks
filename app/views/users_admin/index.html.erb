<% provide(:title, 'All users') %>
<ul class="users">
<html>
<head>
<title><%= yield(:title) %> | Ruby on Rails Study Project</title>
</head>
<body>
<h1> Admin | User List </h1>
<%= link_to "Create New User", new_user_registration_path, class: "btn btn-primary btn-xs active" %>
<table class="table table-striped table-bordered">
   <tr>
   <td align="center"><strong>Name</strong></td>                                                                         
   <td align="center"><strong>Email</strong></td>                                                                           
   <td align="center"><strong>Created At</strong></td>                                                                            
   </tr>      
    <% @users.each do |user| %>
  <tr>
  <td>
    <%= link_to user.name, edit_users_admin_path(user) %> 
  </td>
  
  <td><%= user.email %>
  </td>

  <td>
  <%= user.created_at %>
  </td>
</tr>
<% end %>
</table>
</ul>
</body>
</html>
